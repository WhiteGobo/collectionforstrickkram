		downedges="1"
		>
		<extraoption key="mark" value="startingpoint"/>
		<extraoption key="alternative_stitchtype" value="knit"/>
	</stitchtype>
	<stitchtype name="yarnover_marked"
		manualchar="yomark"
		upedges="1"
		downedges="0"
